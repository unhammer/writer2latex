      </prop>
      <prop oor:name="OptionsPage">
        <value>%origin%/W2LDialogs2/ConfigurationRoot.xdl</value>
      </prop>
      <prop oor:name="EventHandlerService">
        <value>org.openoffice.da.writer2latex.ConfigurationDialog</value>
      </prop>

